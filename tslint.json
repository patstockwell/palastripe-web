{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": true,
    "rules": {
      "no-console": [false],
      "jsx-boolean-value": ["false"],
      "jsx-no-lambda": ["false"],
      "arrow-parens": [false],
      "interface-name": [false],
      "ordered-imports": [false],
      "jsx-alignment": [false],
      "jsx-no-multiline-js": [false],
      "object-literal-sort-keys": [false],
      "quotemark": [false],
      "object-literal-key-quotes": false,
      "trailing-comma": [
        true,
        {
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "functions": "never",
            "typeLiterals": "ignore"
          },
          "esSpecCompliant": true
        }
      ]
    },
    "rulesDirectory": [
      "node_modules/tslint-eslint-rules/dist/rules"
    ]
}
